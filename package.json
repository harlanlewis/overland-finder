{
  "name": "overland-finder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "validate": "node scripts/validate-vehicles.mjs",
    "calc:luxury": "node scripts/calculate-luxury-scores.mjs",
    "calc:offroad": "node scripts/calculate-offroad-scores.mjs",
    "calc:performance": "node scripts/calculate-performance-scores.mjs",
    "calc:all": "npm run calc:offroad && npm run calc:performance && npm run calc:luxury",
    "apply:specs": "node scripts/update-vehicle-specs.mjs",
    "apply:updates": "node scripts/apply-verified-updates.mjs",
    "apply:gen": "node scripts/apply-generation-data.mjs",
    "apply:size": "node scripts/apply-size-data.mjs",
    "apply:body": "node scripts/apply-body-data.mjs",
    "apply:all": "npm run apply:specs && npm run apply:updates && npm run apply:gen && npm run apply:size && npm run apply:body && npm run calc:all",
    "test": "vitest run",
    "test:watch": "vitest",
    "precommit": "npm run validate && npm run test",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "husky": "^9.1.7",
    "vitest": "^3.0.5",
    "vite": "^6.0.7"
  }
}
